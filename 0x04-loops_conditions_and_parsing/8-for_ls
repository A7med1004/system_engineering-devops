#!/usr/bin/env bash
# This script lists files in the current directory,
# displaying only the part of the filename after the first dash

for file in *; do
  # Skip if it's a directory or hidden file
  if [[ "$file" == .* ]]; then
    continue
  fi

  # Extract part after first dash '-'
  # If no dash, print whole name
  if [[ "$file" == *-* ]]; then
    echo "${file#*-}"
  else
    echo "$file"
  fi
done
